#include <bits/stdc++.h>

using namespace std;

const int N = 10;
int p[N];
bool is[N];

void dfs(int u)
{
    if(u == 8) { // 深搜到终点
        for(int i = 0;i < 8;i ++) cout << p[i] << " ";
        cout << endl;
        return ;
    }
    else {
        for(int i = 1;i <= 8;i ++) {
            if(!is[i]) {
                p[u] = i;
                is[i] = true;
                dfs(u + 1);//开始搜
                p[u] = 0;
                is[i] = false;//回溯
            }
        }
    }
}

int main()
{
    dfs(0);
    return 0;
}